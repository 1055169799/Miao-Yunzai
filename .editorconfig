root = true

[*]
# EditorConfig 标准配置项
charset = utf-8
indent_size = 2
indent_style = space
tab_width = 2
insert_final_newline = true
end_of_line = lf
trim_trailing_whitespace = true
max_line_length = off

# IntelliJ 系列配置项
ij_any_space_before_colon = true
ij_any_space_after_colon = true
ij_javascript_use_double_quotes = false
